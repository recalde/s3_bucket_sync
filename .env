# Example .env file for configuring S3BucketSync tool

# Source S3-compatible storage endpoint (NetApp, etc.)
SOURCE_ENDPOINT=https://netapp.example.com

# Destination AWS S3 endpoint
DEST_ENDPOINT=https://s3.amazonaws.com

# Comma-separated list of source bucket names
SOURCE_BUCKET_NAMES=bucket1,bucket2

# Comma-separated list of destination bucket names
DESTINATION_BUCKET_NAMES=bucket1-backup,bucket2-backup

# Comma-separated list of source application names to filter files
SOURCE_APPS=app1,app2

# File copy mode: "memory" for in-memory transfers, "disk" for disk-based transfers
FILE_COPY_MODE=memory

# Source S3 access credentials
SOURCE_ACCESS_KEY=your_source_access_key
SOURCE_SECRET_KEY=your_source_secret_key

# Destination S3 access credentials
DEST_ACCESS_KEY=your_dest_access_key
DEST_SECRET_KEY=your_dest_secret_key

# Date range for copying files (optional)
START_DATE=2023-01-01
END_DATE=2023-12-31

# Maximum number of concurrent file transfers
MAX_CONCURRENCY=5
